@model Young.Web.Models.DynamicTable.DataEditorModel
@{
    ViewBag.Title = "编辑数据";
    Layout = "~/Views/Shared/_WebmasterLayout.cshtml";
}

@section scripts{
    <script type="text/javascript">
      
    </script>
}
<div id="work-panel">
    <div class="module">
        <div class="icon-table title">
            自定义列表：@Model.TableName
        </div>
        @using (Html.BeginForm("editData", "DynamicTable", FormMethod.Post, new { onsubmit = "Young.encodeRichText()" }))
        {
            @Html.Partial("Editor", Model)
        }
    </div>
</div>
